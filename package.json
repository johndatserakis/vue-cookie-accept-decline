{
  "name": "vue-cookie-accept-decline",
  "description": "Show a banner with text, a decline button, and an accept button on your page. Remembers selection with using cookies. Emits an event with current selection on creation. Good for GDPR requirements.",
  "version": "1.0.0",
  "author": "John Datserakis <johndatserakis@gmail.com>",
  "license": "MIT",
  "main": "lib/vue-cookie-accept-decline.min.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "watch": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build:lib": "rm -rf ./lib && bili src/index.vue",
    "build:example": "rm -rf ./dist && cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "tiny-cookie": "^2.1.2",
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "bili": "2.2.6",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "rollup-plugin-vue": "^3.0.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
